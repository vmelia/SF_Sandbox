<apex:page controller="NewCaseListController">
    <apex:pageBlock>
        <apex:repeat var="case" value="{!NewCases}" rendered="true" id="case_list">
            <li>
                <apex:outputLink value="/{!case.ID}">
                    <apex:outputText value="{!case.CaseNumber}"/>                    
                </apex:outputLink>
            </li>
        </apex:repeat>
    </apex:pageBlock>
</apex:page>